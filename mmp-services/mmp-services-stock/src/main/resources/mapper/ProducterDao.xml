<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace 写的就是你要实现的那个接口的路径-->
<mapper namespace="com.aaa.stock.dao.ProducterDao">

    <!-- 根据ID查询厂家信息 -->
    <select id="getAll" resultType="com.aaa.core.entity.Producer">
        select * from stock_producer
    </select>

</mapper>
